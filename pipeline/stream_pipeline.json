{
	"name": "stream_pipeline",
	"properties": {
		"activities": [
			{
				"name": "Copy from Rest to Delta Lake",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:00:10",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"additionalHeaders": {
							"x-api-key": "reqres-free-v1"
						}
					},
					"sink": {
						"type": "AzureDatabricksDeltaLakeSink",
						"preCopyScript": "delete from stockmarket_nasdaq_raw.raw_users",
						"importSettings": {
							"type": "AzureDatabricksDeltaLakeImportCommand",
							"timestampFormat": "yyyy-MM-dd"
						}
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "linkedService1",
							"type": "LinkedServiceReference"
						},
						"path": "landing/stockmarket/nasdaq/__adf_staging",
						"enableCompression": false
					},
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "id",
									"type": "Int"
								}
							},
							{
								"source": {
									"path": "['email']"
								},
								"sink": {
									"name": "email",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['first_name']"
								},
								"sink": {
									"name": "first_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['last_name']"
								},
								"sink": {
									"name": "last_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['avatar']"
								},
								"sink": {
									"name": "avatar",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['data']",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "RestResource1",
						"type": "DatasetReference",
						"parameters": {
							"url": "https://reqres.in/api/users"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureDatabricksDeltaLakeDataset1",
						"type": "DatasetReference",
						"parameters": {
							"database": "stockmarket_nasdaq_raw",
							"table": "raw_users"
						}
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2025-08-11T07:25:23Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}